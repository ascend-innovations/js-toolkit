{
    "name": "@ascend-innovations/eslint-config-ts",
    "version": "4.0.0",
    "description": "Ascend's base vanilla JS w/ Typescript ESLint config",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ascend-innovations/js-toolkit"
    },
    "author": "Ascend Innovations (https://ascend-innovations.com)",
    "contributors": [
        {
            "name": "Chris Hahn",
            "url": "https://github.com/codingwithchris"
        }
    ],
    "bugs": {
        "url": "https://github.com/ascend-innovations/js-toolkit/issues"
    },
    "homepage": "https://github.com/ascend-innovations/js-toolkit",
    "main": "./index.js",
    "scripts": {
        "lint": "eslint . --ext .js,.ts,.tsx,.svelte",
        "lint:fix": "yarn lint --fix",
        "test:js": "eslint __tests__/javascript.js",
        "test:ts": "eslint __tests__/typescript.ts",
        "test:ts:react": "eslint __tests__/react-ts.tsx --config ./react.js",
        "test:ts:svelte": "eslint __tests__/svelte.svelte --config ./svelte.js",
        "test": "yarn test:js && yarn test:ts && yarn test:ts:react && yarn test:ts:svelte"
    },
    "dependencies": {
        "@ascend-innovations/eslint-config": "^2.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.16.10",
        "@babel/eslint-parser": "^7.16.5",
        "@babel/preset-react": "^7.16.7",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-html": "^6.2.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^25.7.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "prettier": "^2.5.1",
        "typescript": "4.5.4",
        "eslint-plugin-svelte3": "^3.4.0",
        "svelte": "^3.46.2"
    },
    "peerDependencies": {
        "@ascend-innovations/eslint-config": "^2.1.0",
        "@babel/core": "^7.16.10",
        "@babel/eslint-parser": "^7.16.5",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-plugin-html": "^6.2.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^25.7.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-svelte3": "^3.4.0",
        "typescript": "4.5.4"
    },
    "peerDependenciesMeta": {
        "eslint-config-airbnb": {
            "optional": true
        },
        "eslint-plugin-jsx-a11y": {
            "optional": true
        },
        "eslint-plugin-react": {
            "optional": true
        },
        "eslint-plugin-react-hooks": {
            "optional": true
        },
        "eslint-plugin-jest": {
            "optional": true
        },
        "eslint-plugin-svelte3": {
            "optional": true
        }
    },
    "volta": {
        "extends": "../../package.json"
    }
}
