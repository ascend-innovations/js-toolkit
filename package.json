{
    "name": "ascend-eslint-configs",
    "version": "0.0.0",
    "description": "Ascend's ESLint config for writing clean, consistent code",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ascend-innovations/eslint-config-ascend"
    },
    "keywords": [
        "lint",
        "eslint",
        "eslint config",
        "eslint react",
        "config",
        "ascend",
        "javascript",
        "react",
        "typescript",
        "eslint typescript",
        "tslint",
        "prettier",
        "ecmascript",
        "eslint svelte",
        "svelte"
    ],
    "author": "Ascend Innovations (https://ascend-innovations.com)",
    "contributors": [
        {
            "name": "Chris Hahn",
            "url": "https://github.com/codingwithchris"
        }
    ],
    "bugs": {
        "url": "https://github.com/ascend-innovations/eslint-config-ascend/issues"
    },
    "homepage": "https://github.com/ascend-innovations/eslint-config-ascend",
    "scripts": {
        "bootstrap": "lerna bootstrap",
        "lint": "eslint .",
        "lint:fix": "eslint --fix",
        "test": "echo \"No test specified\"",
        "clean:nm": "lerna clean && rimraf node_modules",
        "clean:nuke": "rimraf yarn.lock && yarn clean:nm",
        "clean:reset": "yarn clean:nuke && yarn install"
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "lerna": "^4.0.0",
        "lerna-update-wizard": "^0.17.8",
        "rimraf": "^3.0.2"
    },
    "volta": {
        "node": "16.10.0",
        "yarn": "1.22.11"
    },
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "multi-semantic-release": "^2.9.0"
    }
}
